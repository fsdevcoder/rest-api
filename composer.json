{
  "name": "Custom Mvc",
  "type": "project",
  "description": "The MVC pattern.",
  "keywords": [
    "php",
    "mvc"
  ],
  "license": "MIT",
  "config": {
    "platform": {
      "php": "7.3.11"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    },
    "classmap": ["app"]
  },
  "require": {
    "ext-pdo": "*",
    "ext-json": "*",
    "php-di/php-di": "^6.0",
    "phpunit/phpunit": "^9.0",
    "symfony/console": "^4.0"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^4.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
